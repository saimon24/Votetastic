{
  "arrow-body-style": "off",
  "bracketSpacing": true,
  "prefer-arrow-callback": "off",
  "printWidth": 120,
  "singleQuote": true,
  "semi": true,
  "trailingComma": "es5",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.component.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
